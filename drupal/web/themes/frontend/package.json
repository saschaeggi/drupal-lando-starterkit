{
  "name": "drupal-frontend",
  "version": "2.0.0",
  "description": "Frontend Starterkit for Drupal Projects",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --progress --mode development --config  webpack.config.dev.js --watch",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "lint": "lint-staged",
    "lint-all": "eslint \"src/**/*.js\" --fix",
    "pre-commit": "lint-staged"
  },
  "email": "hello@saschaeggenberger.com",
  "author": "Sascha Eggenberger",
  "license": "MIT",
  "homepage": "https://sascha.is",
  "keywords": [
    "drupal",
    "webpack",
    "frontend",
    "boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "bourbon": "^5.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.1",
    "csscomb": "4.3.0",
    "es6-promise": "^4.1.1",
    "eslint": "6.8.0",
    "eslint-import-resolver-webpack": "0.12.0",
    "eslint-plugin-import": "2.19.1",
    "expose-loader": "^0.7.5",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.1",
    "lint-staged": "9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "svg-inline-loader": "^0.8.0",
    "svg-spritemap-webpack-plugin": "=3.3.7",
    "webpack": "^4.41.3",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "^3.3.10",
    "webpack-fix-style-only-entries": "^0.4.0",
    "webpack-merge": "^4.2.2",
    "webpack-modernizr-loader": "^5.0.0",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "@unic/composite-observer": "0.0.10",
    "core-js": "^2.6.11",
    "mdn-polyfills": "^5.19.0",
    "modernizr": "^3.8.0",
    "node-sass-glob-importer": "^5.3.0",
    "normalize.css": "^8.0.0",
    "vanilla-lazyload": "^12.4.0"
  },
  "lint-staged": {
    "*.scss": [
      "csscomb --tty-mode",
      "git add"
    ],
    "*.js": [
      "eslint --fix \"{,**,**/**}*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "github.com/saschaeggi/drupal-docksal-starterkit"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "ie > 10",
    "iOS > 9"
  ]
}
